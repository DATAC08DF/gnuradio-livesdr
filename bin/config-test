#!/bin/sh
set -o nounset
#set -x #DEBUG

. config/config-vars

for cmd in \
    $CURL \
    $GPG \
    $SHA256SUM \
    $MKSQUASHFS \
    $MD5SUM \
    $MKISOFS \
    ; do

    if ! test -x $cmd ; then
        echo ERROR: $cmd not found or not executable
        echo Try running 'make deps'.
        exit 1
    fi
done

exit 0
